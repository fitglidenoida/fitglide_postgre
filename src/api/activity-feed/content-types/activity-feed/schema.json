{
  "kind": "collectionType",
  "collectionName": "activity_feeds",
  "info": {
    "singularName": "activity-feed",
    "pluralName": "activity-feeds",
    "displayName": "activity-feed",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "workout",
        "achievement",
        "pack_update",
        "daily_activity",
        "live_workout"
      ],
      "required": true
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "data": {
      "type": "json",
      "required": true
    },
    "visibility": {
      "type": "enumeration",
      "enum": [
        "friends",
        "packs",
        "public",
        "private"
      ],
      "required": true,
      "default": "friends"
    },
    "sharedWith": {
      "type": "json"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "activity_feeds"
    },
    "cheers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cheer.cheer",
      "mappedBy": "activity_feed"
    },
    "isLive": {
      "type": "boolean",
      "default": false
    },
    "workoutId": {
      "type": "string"
    },
    "sessionId": {
      "type": "string"
    },
    "sessionStartTime": {
      "type": "datetime"
    },
    "sessionEndTime": {
      "type": "datetime"
    },
    "activityDate": {
      "type": "date"
    },
    "duration": {
      "type": "integer"
    },
    "distance": {
      "type": "decimal"
    },
    "calories": {
      "type": "decimal"
    },
    "cheerCount": {
      "type": "integer"
    },
    "commentCount": {
      "type": "integer"
    },
    "lastLiveUpdate": {
      "type": "datetime"
    },
    "liveData": {
      "type": "json"
    }
  }
}
