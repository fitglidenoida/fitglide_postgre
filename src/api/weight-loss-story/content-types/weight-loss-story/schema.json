{
  "kind": "collectionType",
  "collectionName": "weight_loss_stories",
  "info": {
    "singularName": "weight-loss-story",
    "pluralName": "weight-loss-stories",
    "displayName": "WeightLossStory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "createdAt": {
      "type": "datetime"
    },
    "storyId": {
      "type": "string"
    },
    "thenPhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "nowPhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "thenWeight": {
      "type": "integer"
    },
    "nowWeight": {
      "type": "integer"
    },
    "weightLost": {
      "type": "integer"
    },
    "storyText": {
      "type": "text"
    },
    "likes": {
      "type": "integer"
    },
    "visibility": {
      "type": "enumeration",
      "enum": [
        "Everyone",
        "Friends",
        "Pack"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "weight_loss_stories"
    },
    "firstName": {
      "type": "string"
    },
    "storyType": {
      "type": "enumeration",
      "enum": [
        "Weight Loss",
        "Workout",
        "Transformation",
        "Hydration",
        "Sleep",
        "Mindfulness",
        "Consistency",
        "Other"
      ]
