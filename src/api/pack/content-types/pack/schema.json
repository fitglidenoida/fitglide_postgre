{
  "kind": "collectionType",
  "collectionName": "packs",
  "info": {
    "singularName": "pack",
    "pluralName": "packs",
    "displayName": "pack"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "gliders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "packs"
    },
    "captain": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "pack"
    },
    "goal": {
      "type": "integer",
      "required": true
    },
    "progress": {
      "type": "integer"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "pack"
    }
  }
}
